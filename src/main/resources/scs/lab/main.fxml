<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scs.lab.ViewFXMLController">
   <children>
      <VBox layoutX="9.0" layoutY="14.0" prefHeight="377.0" prefWidth="571.0" spacing="15.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="27.0" prefWidth="571.0">
               <children>
                  <Label text="Benchmarks for Testing Multicore Performace">
                     <font>
                        <Font name="Arial Black" size="15.0" />
                     </font>
                  </Label>
                  <Pane prefHeight="27.0" prefWidth="50.0" />
                  <Label alignment="BOTTOM_CENTER" prefHeight="19.0" prefWidth="72.0" text="CPU Usage: ">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font></Label>
                  <Label fx:id="cpuUsageLabel" alignment="BOTTOM_LEFT" prefHeight="19.0" prefWidth="51.0">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font></Label>
               </children>
            </HBox>
            <VBox prefHeight="71.0" prefWidth="571.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Prime Finder Test">
                           <font>
                              <Font name="Arial Italic" size="14.0" />
                           </font></Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Thread number: " />
                        <TextField fx:id="threadNoPrimeFinder" prefHeight="25.0" prefWidth="68.0" />
                        <Pane prefHeight="26.0" prefWidth="101.0" />
                        <Label fx:id="primeTestStatus" prefHeight="18.0" prefWidth="158.0" text="Label" />
                        <Pane prefHeight="26.0" prefWidth="15.0" />
                        <Label text="Time taken: " />
                        <Label fx:id="timeTakenPrimeFinderLabel" prefHeight="17.0" prefWidth="67.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="For fastest result: 32 threads" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="71.0" prefWidth="571.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Pi Calculation test">
                           <font>
                              <Font name="Arial Italic" size="14.0" />
                           </font></Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Thread number: " />
                        <TextField fx:id="threadNoPiCalc" prefHeight="25.0" prefWidth="68.0" />
                        <Pane prefHeight="26.0" prefWidth="101.0" />
                        <Label fx:id="piCalcTestStatus" prefHeight="18.0" prefWidth="158.0" text="Label" />
                        <Pane prefHeight="26.0" prefWidth="15.0" />
                        <Label text="Time taken: " />
                        <Label fx:id="timeTakenPiCalcLabel" prefHeight="17.0" prefWidth="67.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="For fastest result: ~6 threads" />
                     </children>
                  </HBox>
               </children>
            </VBox>

            <VBox prefHeight="71.0" prefWidth="571.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Merge Sort test">
                           <font>
                              <Font name="Arial Italic" size="14.0" />
                           </font></Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Thread number: " />
                        <TextField fx:id="threadNoMergeSort" prefHeight="25.0" prefWidth="68.0" />
                        <Pane prefHeight="26.0" prefWidth="101.0" />
                        <Label fx:id="mergeSortTestStatus" prefHeight="18.0" prefWidth="158.0" text="Label" />
                        <Pane prefHeight="26.0" prefWidth="15.0" />
                        <Label text="Time taken: " />
                        <Label fx:id="timeTakenMergeSortLabel" prefHeight="17.0" prefWidth="67.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="For fastest result: ~6 threads" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Button mnemonicParsing="false" onAction="#startBenchmarks" text="Start benchmarks" />
         </children>
      </VBox>
   </children>
</AnchorPane>
